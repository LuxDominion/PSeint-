Algoritmo regla_de_tres_simple
	Escribir "Ingresa tu primer valor que multiplicará"
	Leer p
	Escribir "Ingresa tu segundo valor que multiplicará"
	Leer ppv
	Escribir "Ingresa el valor que dividirá"
	Leer ppc
	resultado<-(p*ppv)/ppc
	Escribir "El resultado de tu operación es " , resultado ;
FinAlgoritmo
